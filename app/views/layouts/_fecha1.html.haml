-if eti == nil

	= text_field_tag 'fin','', :class=>'datepicker input-medium'

	:javascript
		$('.datepicker').datepicker({
		"format": "dd/mm/yyyy",
		"weekStart": 1,
		"autoclose": true,
		"language": "es"});

		$('#fin').val(fechaAhora());
- else
	= text_field_tag 'fin',eti, :class=>'datepicker input-medium'

	:javascript
		$('.datepicker').datepicker({
		"format": "dd/mm/yyyy",
		"weekStart": 1,
		"autoclose": true,
		"language": "es"});
